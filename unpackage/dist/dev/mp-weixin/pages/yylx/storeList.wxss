.content {
  background-color: #f2f2f2;
  overflow: auto;
  overflow-x: hidden;
}
.content .search_box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 40rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.content .search_box .address input {
  width: 540rpx;
  height: 70rpx;
  background-color: #FFFFFF;
  -webkit-border-radius: 35rpx;
          border-radius: 35rpx;
  font-size: 26rpx;
  color: #000;
  margin-top: 20rpx;
  padding-left: 90rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.content .search_box .search {
  padding: 0;
  margin: 0;
  width: 110rpx;
  height: 70rpx;
  background: url("https://xcxsite.vjifen.com/v/static/qp2020/yylx/button_search.png") no-repeat center;
  background-size: 100% auto;
}
.storeList {
  width: 100%;
  padding: 0 40rpx;
  background-color: #FFFFFF;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  margin-top: 20rpx;
}
.storeList .store {
  border-bottom: 1px #e5e5e5 solid;
  background: url("https://xcxsite.vjifen.com/v/static/qp2020/yylx/icon_right.png") no-repeat right;
  background-size: 20rpx auto;
  padding: 25rpx 0;
}
.storeList .store .storeName {
  color: #000;
  font-size: 30rpx;
}
.storeList .store .storeName text {
  font-size: 26rpx;
}
.storeList .store > text {
  font-size: 22rpx;
  color: #979797;
}
.storeList .store:nth-last-of-type(1) {
  border: 0;
}
view.opensetting {
  width: 100%;
  height: 500rpx;
  background: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
}
view.opensetting .head {
  height: 120rpx;
  line-height: 120rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #333;
  position: relative;
}
view.opensetting .head image.headimg {
  width: 60rpx;
  height: 60rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  margin: 30rpx 0 30rpx 30rpx;
}
view.opensetting .head image.close {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 40rpx;
  right: 40rpx;
}
view.opensetting .head text {
  padding: 0 20rpx;
}
view.opensetting .loc_content text:nth-of-type(1) {
  font-size: 40rpx;
  color: #000;
  font-weight: bold;
  padding: 30rpx;
}
view.opensetting .loc_content text:nth-of-type(2) {
  font-size: 30rpx;
  padding: 0 30rpx;
  color: #333;
  line-height: 80rpx;
  display: block;
}
view.opensetting > button {
  width: 400rpx;
  height: 85rpx;
  background: #04c05f;
  display: block;
  margin: 50rpx auto;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  color: #fff;
  font-size: 32rpx;
}
view.location_fail {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 2;
  background: rgba(0, 0, 0, 0.8);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  overflow: hidden;
}
view.location_fail image {
  width: 80%;
  display: block;
}

