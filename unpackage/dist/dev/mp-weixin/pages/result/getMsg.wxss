.content {
  background: url("https://xcxsite.vjifen.com/v/static/qp2020/bg.png?v=2") no-repeat top;
  background-size: 100% auto;
  position: relative;
}
#wrap {
  color: #fff;
}
#wrap text.black {
  display: block;
  text-align: center;
  padding: 0 100rpx;
  font-size: 28rpx;
  margin: 300rpx auto;
}
#wrap .tip {
  line-height: 50rpx;
  text-align: center;
  padding: 160rpx 0 100rpx;
}
#tel_box {
  height: 80rpx;
  line-height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-top: 1px #ccc solid;
  border-bottom: 1px #ccc solid;
}
#tel_box input {
  height: 80rpx;
  line-height: 80rpx;
  width: 80%;
  font-size: 28rpx;
  padding-left: 50rpx;
}
#yz_box {
  height: 80rpx;
  line-height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-bottom: 1px #ccc solid;
}
#yz_box input {
  height: 80rpx;
  line-height: 80rpx;
  width: 60%;
  padding-left: 50rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
#get_yz {
  width: 40%;
  text-align: center;
  border-left: 1px #ccc solid;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
#yz_box {
  bottom: 0;
}
#tj {
  margin-top: 72rpx;
  display: block;
  margin: 100rpx auto;
}
#tips {
  text-align: center;
}

